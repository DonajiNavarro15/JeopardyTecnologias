        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JeopardyGame.Views"
        mc:Ignorable="d"
        Title="UserManagerWindow" Height="770" Width="1300">
    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
        <Image HorizontalAlignment="Left" Height="135" Width="274" Margin="64,10,0,0" VerticalAlignment="Top" Source="/Images/JeopardyFosfoSign.jpg"/>
        <Frame x:Name="CurrentPage" NavigationUIVisibility="Hidden" />
        <Grid.Background>
            <ImageBrush ImageSource="/Images/BackGround.jpg"  Stretch="UniformToFill"/>
